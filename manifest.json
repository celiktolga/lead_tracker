{
	"manifest_version": 3,
	"version": "1.0",
	"name": "Leads tracker",
	"description": "This extension collect browser tabs for tracker.",
	"action": {
		"default_popup": "index.html",
		"default_icon": "icon.png"
	},
	"permissions": [
		"tabs"
	],
	"icons": { 
		"16": "icon.png",
        "48": "icon.png",
        "128": "icon.png" 
	}
}